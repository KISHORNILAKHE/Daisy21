<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="3">
	<listeners>
		<listener class-name="com.testingshastra.util.TestListener"></listener>
	</listeners>
	<test name="Chrome">
		<parameter name="browser-name" value="Chrome"></parameter>
		<classes>
			<class name="com.testingshastrasignuptests.HomePageTests" />
		</classes>
	</test> <!-- Test -->
	<test name="Firefox">
		<parameter name="browser-name" value="Firefox"></parameter>
		<classes>
			<class name="com.testingshastrasignuptests.HomePageTests" />
		</classes>
	</test> <!-- Test -->
	<test name="IE">
		<parameter name="browser-name" value="ie"></parameter>
		<classes>
			<class name="com.testingshastrasignuptests.HomePageTests" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
